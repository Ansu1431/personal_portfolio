# Example .env file for personal_portfolio
# DO NOT put real secrets in this file. Replace values locally or copy to a private `.env`.

# MySQL configuration
DB_HOST=your-db-host.example.com
DB_USER=your_db_user
DB_PASS=your_db_password
DB_NAME=portfolio_db
# Optional: DB_PORT=3306

# JWT secret (generate a secure random value for production)
JWT_SECRET=ReplaceWithASecureRandomString

# Optional S3 / object storage placeholders (used only if you change uploads)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=us-east-1
# S3_BUCKET=your-bucket-name

# Optional API base used by frontend when you want baked-in API URL
# API_BASE=https://your-backend.example.com
